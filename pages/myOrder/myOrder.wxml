<view class="page">
  <view class="page__bd">
    <block wx:for="{{orderList}}" wx:key="index">
      <view class="weui-panel weui-panel_access">
        <view class="weui-panel__bd">
          <view url="" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
            <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image class="weui-media-box__thumb" src="{{item.img}}" />
            </view>
            <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
              <view class="weui-media-box__title">{{item.name}}</view>
              <view class="weui-media-box__desc">{{item.dateStart}}è‡³{{item.dateEnd}}</view>
              <view class="weui-media-box__desc">{{item.price}}</view>
            </view>
          </view>
        </view>
      </view>
    </block>

  </view>
</view>